#include <iostream>
using namespace std;
int main()
{
	int age,n;
	cout<<"Enter the age of the person: ";
	cin>> age;
	if (age>=18)
	{
		cout<<"You are eligible for voting.";
	}
	else if((age<18)&&(age>0))
	{
    n=18-age;
	cout<<"You eligible for voting after"<<n<<"years";
	}
	else
	{
		cout<<"enetered age is invalid";
	}
  return 0;
}
